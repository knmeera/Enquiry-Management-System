@model IEnumerable<Enquiry.Models.Enquiry>
@using System.Linq
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_WebLayout.cshtml";
}

<h2>List Of Enquiries </h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Last_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Emailid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mobilenumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EducationQualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Experience)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Domain)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoursesId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChanelId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SourcesId)
        </th>
        <th></th>
    </tr>
    @if (Model.Any())
    {
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.First_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Last_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Emailid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mobilenumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EducationQualification)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Experience)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Domain)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoursesId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChanelId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SourcesId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EnquiryId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EnquiryId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EnquiryId })
        </td>
    </tr>
}
}else
    {
        <h5>Sorry.! no data found</h5>
    }

</table>
